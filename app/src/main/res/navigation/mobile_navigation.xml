<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="kr.ac.cbnu.saengsaengyaktong.ui.medicine.MedicineListFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_medicine_list" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="kr.ac.cbnu.saengsaengyaktong.ui.map.MapFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_community"
        android:name="kr.ac.cbnu.saengsaengyaktong.ui.notifications.NotificationsFragment"
        android:label="@string/title_community"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_profile"
